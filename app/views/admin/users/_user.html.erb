<td><%= count + 1 %></td>
<td><%= user.name %></td>
<td><%= user.email %></td>
<td><%= user.address %></td>
<td>
	<%= link_to admin_user_path(user) do %>
		<span class="glyphicon glyphicon-zoom-in show-user" ></span>
	<% end %>
	<%= link_to [:admin, user], method: :delete, remote: true, data: {confirm: "Are you sure delete #{user.name}"} do %>
		<span class="glyphicon glyphicon-trash"></span>
	<% end %>
</td>